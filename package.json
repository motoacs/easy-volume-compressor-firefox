    "dev": "npm run build && web-ext run --source-dir ./"
  },
  "repository": {
    "type": "git",
    "url": "git+https://motoacs@github.com/motoacs/easy-volume-compressor-firefox.git"
  },
  "keywords": [
    "firefox-addon",
    "audio",
    "compressor",
    "volume"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/motoacs/easy-volume-compressor-firefox/issues"
  },
  "homepage": "https://github.com/motoacs/easy-volume-compressor-firefox#readme",
  "devDependencies": {
    "@types/firefox-webext-browser": "^120.0.4",
    "ts-loader": "^9.5.2",
    "typescript": "^5.7.3",
    "web-ext": "^8.4.0",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1"
  }
}
